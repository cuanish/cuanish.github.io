    <script>
        document.addEventListener('DOMContentLoaded', () => {
            // Add smooth loading animation
            const content = document.querySelector('.section-content');
            content.style.opacity = '0';
            content.style.transform = 'translateY(20px)';
            
            setTimeout(() => {
                content.style.transition = 'all 0.6s ease-out';
                content.style.opacity = '1';
                content.style.transform = 'translateY(0)';
            }, 100);
            
            // Add staggered animation to links
            const links = document.querySelectorAll('.links-list li');
            links.forEach((link, index) => {
                link.style.opacity = '0';
                link.style.transform = 'translateY(10px)';
                
                setTimeout(() => {
                    link.style.transition = 'all 0.4s ease-out';
                    link.style.opacity = '1';
                    link.style.transform = 'translateY(0)';
                }, 200 + (index * 100));
            });
            
            // Add click tracking for analytics (optional)
            links.forEach(link => {
                const anchor = link.querySelector('a');
                if (anchor) {
                    anchor.addEventListener('click', () => {
                        const title = anchor.querySelector('.link-title').textContent;
                        console.log(`Clicked resource: ${title}`);
                    });
                }
            });
        });
